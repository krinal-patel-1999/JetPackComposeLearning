<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.tweetesAppWithMVVMAndFlow.Tweetsy"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetPackComposeLearning"
        tools:targetApi="31">
        <activity
            android:name="com.tweetesAppWithMVVMAndFlow.TweetsyAppActivity"
            android:exported="true"
            android:label="@string/title_activity_tweetsy_app"
            android:theme="@style/Theme.JetPackComposeLearning">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.quotesAppListing.quotesApp.QuotesActivity"
            android:exported="false"
            android:label="@string/title_activity_quotes"
            android:theme="@style/Theme.JetPackComposeLearning">
            <!--<intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>-->
        </activity>
        <activity
            android:name=".ExRowCol"
            android:exported="false"
            android:label="@string/title_activity_ex_row_col"
            android:theme="@style/Theme.JetPackComposeLearning" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JetPackComposeLearning">

            <!--
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            -->
        </activity>
    </application>

</manifest>